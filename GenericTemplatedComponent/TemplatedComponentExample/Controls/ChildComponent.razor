@typeparam TItem
<h3>ChildComponent</h3>

<div>
    @foreach (TItem p in Items)
    {
        @MySection(p)
    }


    <br />
    <br />
    <br />


</div>

@code {

           
    [Parameter]
    public List<TItem> Items { get; set; }


    [Parameter]
    public RenderFragment<TItem> MySection { get; set; }



}
